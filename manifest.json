{
  "manifest_version": 3,
  "name": "OpenAI Log Line Breaks",
  "version": "1.0",
  "description": "Show whitespace in prompts in OpenAI log views",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://platform.openai.com/logs/*"],
      "js": ["content.js"]
    }
  ]
}